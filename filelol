local set_hidden_prop = set_hidden_prop or sethiddenproperty or nil 

local set_simulation_radius = set_simulation_radius or setsimulationradius or function(ply,val) set_hidden_prop(ply,"SimulationRadius",val) end

if set_hidden_prop == nil then 
	warn("UR EXPLOIT DONT WORK WITH SETHIDDENPROPERTY.. SO IT DONT WORK..")
end

local function g_n_o()
	settings().Physics.AllowSleep = false
	set_hidden_prop(game.Players.LocalPlayer,"MaximumSimulationRadius",math.huge)
	local areWeSynapse = setsimulationradius or nil 
	if areWeSynapse == nil then
		set_simulation_radius(game.Players.LocalPlayer,math.huge)
	else 
		set_simulation_radius(math.huge)
	end
end

local function tellClient(msg)
	local sgui = Instance.new("ScreenGui")
	sgui.ResetOnSpawn = false 

	local txtLabel = Instance.new("TextLabel")
	txtLabel.AnchorPoint = Vector2.new(0.5,0.5)
	txtLabel.Size = UDim2.new(0.7,0,0.4,0)
	txtLabel.Position = UDim2.new(0.5,0,0.3,0)

	txtLabel.BackgroundTransparency = 1

	txtLabel.Text = msg 
	txtLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	txtLabel.TextStrokeTransparency = 0

	txtLabel.TextScaled = true 

	txtLabel.Font = Enum.Font.GothamBlack
	txtLabel.Parent = sgui 


	sgui.Parent = game.Players.LocalPlayer.PlayerGui 

	local bind = Instance.new("BindableEvent")
	bind.Event:Connect(function ()
		for i = 1 , 200 do game:GetService("RunService").Heartbeat:Wait() end 
		local TweenService = game:GetService("TweenService")

		local t = TweenService:Create(txtLabel,TweenInfo.new(2),{TextTransparency = 1, TextStrokeTransparency = 1})
		t.Completed:Connect(function ()
			sgui:Destroy()
			t:Destroy()
		end)

		t:Play()
	end)	

	bind:Fire()
	bind:Destroy()
end
